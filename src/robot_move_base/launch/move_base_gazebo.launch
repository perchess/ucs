<launch>
  <master auto="start"/>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <!--<param name="base_local_planner" value="base_local_plannerj/TrajectoryPlannerROS" /> -->

    <!--<param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />-->
<!--    <rosparam file="$(find robot_move_base)/config/gazebo/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robot_move_base)/config/gazebo/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robot_move_base)/config/gazebo/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/gazebo/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/gazebo/base_local_planner_params.yaml" command="load" />-->

    <rosparam file="$(find robot_move_base)/config/itmo/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robot_move_base)/config/itmo/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robot_move_base)/config/itmo/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/itmo//global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/gazebo/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/itmo/teb/teb_local_planner_params.yaml" command="load" />
    <rosparam file="$(find robot_move_base)/config/itmo/base_global_planner_params.yaml" command="load" />


    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
<!--    <param name="base_global_planner" value="carrot_planner/CarrotPlanner" />-->
    <param name="controller_frequency" value="5.0" />
    <param name="controller_patience" value="15.0" />
    <param name="max_planning_retries" value="10" />
    <param name="oscillation_timeout" value="10.0" />
    <param name="clearing_rotation_allowed" value="false" /> <!-- Our carlike robot is not able to rotate in place -->

    <remap from="cmd_vel" to="/scooter/cmd_vel" />
    <remap from="odom" to="/scooter/odom" />
  </node>
</launch>
